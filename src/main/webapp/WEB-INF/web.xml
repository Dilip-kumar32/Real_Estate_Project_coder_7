<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletLogin</servlet-name>
  	<display-name>ServletLogin</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ServletLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveServlet</servlet-name>
  	<display-name>SaveServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.SaveServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletLogout</servlet-name>
  	<display-name>ServletLogout</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ServletLogout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletSearch</servlet-name>
  	<display-name>ServletSearch</display-name>
  	<description></description>
  	<servlet-class>com.servlets.ServletSearch</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletLogin</servlet-name>
  	<url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveServlet</servlet-name>
  	<url-pattern>/SaveServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletLogout</servlet-name>
  	<url-pattern>/ServletLogout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletSearch</servlet-name>
  	<url-pattern>/ServletSearch</url-pattern>
  </servlet-mapping>
</web-app>
